{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base/legacy",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint",
        "plugin:jest/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "rules": {
    }
}